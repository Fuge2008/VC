<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              xmlns:tools="http://schemas.android.com/tools"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_enfragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="32dp"
        android:orientation="vertical">

        <include layout="@layout/person_style"/>

        <!--<include layout="@layout/regis2_style2"/>-->

        <com.saas.saasuser.slidecode.SwipeCaptchaView
            android:layout_marginTop="6dp"
            android:layout_gravity="center"
            android:id="@+id/swipeCaptchaView"
            android:layout_width="match_parent"
            android:layout_height="160dp"
            android:layout_centerHorizontal="true"
            android:scaleType="centerCrop"
            tools:src="@drawable/pic11"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"
            app:captchaHeight="30dp"
            app:captchaWidth="30dp"/>

        <SeekBar
            android:id="@+id/dragBar"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_below="@id/swipeCaptchaView"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="12dp"
            android:progressDrawable="@drawable/dragbg"
            android:thumb="@drawable/thumb_bg"/>


        <!-- <TextView
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:text="VV租行管理"
             android:layout_gravity="right"
             android:textSize="17sp"
             android:layout_marginRight="20dp"
             android:layout_marginTop="10dp"/>-->


        <Button
            android:id="@+id/btn_en_register"
            style="@style/button_next"
            android:text="下一步"/>

    </LinearLayout>

    <!---->


</LinearLayout>