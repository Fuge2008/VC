+		}
+		if (enable) {
+			mGraspPolyline.setVisible(true);
+			mGraspStartMarker.setVisible(true);
+			mGraspEndMarker.setVisible(true);
+			mGraspRoleMarker.setVisible(true);
+		} else {
+			mGraspPolyline.setVisible(false);
+			mGraspStartMarker.setVisible(false);
