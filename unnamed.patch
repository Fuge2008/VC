+					toast("ssdk_facebookmessenger_client_inavailable");
+				} else {
+					toast("ssdk_oks_share_failed");
+				}
+			} break;
+			case 3: {
+				// 取消
+				toast("ssdk_oks_share_canceled");
+			} break;
+		}
