+                            .getSystemService(Context.WIFI_SERVICE);
+            if (wifiManager.isWifiEnabled()) {
+                    enable = true;
+                    Log.i(Thread.currentThread().getName(), "isWifiEnabled");
+            }
+             
+            Log.i(Thread.currentThread().getName(), "isWifiDisabled");
+            return enable;
+    }
