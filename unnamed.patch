+	 * @param context
+	 * @param parent
+	 * @param layoutId
+	 * @param position
+	 */
+	private BaseViewHolder(Context context, ViewGroup parent, int layoutId,
+                           int position) {
+		this.mViews = new SparseArray<View>();
+		this.mConvertView = LayoutInflater.from(context).inflate(layoutId,
+				parent, false);
