+	}
+
+	public AsyncRequest(int requestCode, boolean isCheckNetwork, OnDataListener listener) {
+		this.requestCode = requestCode;
+		this.isCheckNetwork = isCheckNetwork;
+		this.listener = listener;
+	}
+
+	public int getRequestCode() {
