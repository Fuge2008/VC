+    public static String filterNull(String str, String _default, String tail){
+
+        if (str == null || "".equals(str) || "null".equalsIgnoreCase(str)) {
+            return _default;
+        }else {
+            if(tail != null) {
+                str += tail;
+            }
+            return str;
+        }
+    }
