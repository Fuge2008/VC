+
+        //行业
+        try {
+            StringBuffer buffer = new StringBuffer();
+            InputStream inputStream = getAssets().open("lus.json");
+            byte[] bytes = new byte[1024];
+            int length = -1;
+            while ((length = inputStream.read(bytes)) != -1){
+                buffer.append(new String(bytes, 0, length, "utf-8"));
+            }
