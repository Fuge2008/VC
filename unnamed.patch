+        mCalendarHolder.animate(progress);
+        mWeeksHolder.animate(progress);
+
+        // animate views if necessary
+        if (mViews != null) {
+            for (AbstractViewHolder view : mViews) {
+                view.animate(progress);
+            }
+        }
+
+        // request layout
+        mCalendarView.requestLayout();
+
+    }
